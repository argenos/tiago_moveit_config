<launch>
  <arg name="robot_namespace"         default=""/>

  <rosparam command="load" file="$(find tiago_moveit_config)/config/sensors_rgbd.yaml" />

  <param name="octomap_frame" type="string" value="$(arg robot_namespace)/odom" />
  <param name="octomap_resolution" type="double" value="0.05" />
  <param name="max_range" type="double" value="5.0" />

</launch>
